{
  "name": "@digit-ui/digit-ui-components",
  "version": "1.0.1",
  "license": "MIT",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://unified-dev.digit.org/storybook/",
  "author": "Jagankumar <jagan.kumar@egov.org.in>",
  "scripts": {
    "example": "cd example && npm run start",
    "build-webpack": "webpack --mode=production --node-env=production",
    "build": "yarn build-webpack",
    "publish": "webpack --config webpack.config.js && npm publish --tag mfe",
    "build-old": "microbundle-crl --compress --no-sourcemap --format cjs",
    "starts": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "yarn build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public -o dist-storybook",
    "deploy-storybook": "npm run build-storybook && surge --project dist-storybook --domain react-components-$npm_package_version.surge.sh",
    "start": "webpack serve --config webpack.config.js"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.7.3",
    "gh-pages": "2.2.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.6.1",
    "react-responsive": "9.0.2",
    "react-router-dom": "5.3.0",
    "react-scripts": "^4.0.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "5.7.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@cyntler/react-doc-viewer": "1.10.3",
    "@digit-ui/digit-ui-libraries-mfe": "1.0.17",
    "@digit-ui/digit-ui-svg-components": "1.0.1",
    "@googlemaps/js-api-loader": "1.13.10",
    "autoprefixer": "^10.4.15",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react-date-range": "1.3.0",
    "react-drag-drop-files": "2.3.10",
    "react-hook-form": "6.15.8",
    "react-i18next": "11.16.2",
    "react-lottie": "1.2.4",
    "react-table": "7.7.0",
    "react-webcam": "7.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "digit",
    "core",
    "components",
    "dpg"
  ]
}
